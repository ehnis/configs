{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-remote",
        "address": "local",
        "detour": "direct"
      }
    ]
  },
  "inbounds": [
    {
      "domain_strategy": "",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "sniff": true,
      "sniff_override_destination": false,
      "tag": "mixed-in",
      "type": "mixed"
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "address": "172.19.0.1/28",
      "mtu": 9000,
      "auto_route": true,
      "stack": "system"
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "endpoints": [
    {
      "system": true,
      "name": "wg0",
      "address": "10.8.1.3/32",
      "private_key": "IEx78UB6Uv07sHQCw2I6cXMc+AHMSEKTkEHjUcQxsZ4=",
      "type": "wireguard",
      "tag": "proxy",
      "peers": [
        {
          "address": "94.159.107.28",
          "port": 41304,
          "public_key": "7h9CG/tR7Ne6Zd/qCzkT+HQskXjS7INGplA/hPWdqm4=",
          "pre_shared_key": "X5Y1KiynO3TY3uyRARFHLVOL4v410Ic3GZx0vp5SbMM=",
          "allowed_ips": [
            "0.0.0.0/0",
            "::/0"
          ]
        }
      ]
    }
  ],
  "route": {
    "final": "direct",
    "auto_detect_interface": true,
    "rules": [
      {
        "action": "sniff"
      },
      {
        "inbound": [
          "mixed-in"
        ],
        "outbound": "proxy"
      },
      {
        "inbound": [
          "tun-in"
        ],
        "process_path_regex": [
          "^.*(wget|git|electron|firefox|firefox-bin|flatpak|.flatpak-wrappe|.flatpak-wrapped|node)$"
        ],
        "outbound": "proxy"
      },
      {
        "inbound": [
          "tun-in"
        ],
        "domain_suffix": [
          "googlevideo.com",
          "youtu.be",
          "youtube.com",
          "ytimg.com",
          "ggpht.com",
          "gstatic.com",
          "kinobar.ai",
          "discord.com",
          "discord.gg",
          "discordapp.com",
          "discordapp.net"
        ],
        "outbound": "proxy"
      },
      {
        "port": 53,
        "process_name": [
          "anything in here, like really anything"
        ],
        "action": "hijack-dns"
      }
    ]
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    }
  }
}
